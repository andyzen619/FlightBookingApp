package databases;

/**
 * Indicates an attempt to find a user using an email that doesn't exist.
 * 
 * @author Raphael Alviz, Zain Amir, Ian Ferguson, Andy Liang, Johnathan Tsoi
 */
public class NoSuchEmailException extends Exception {

  /**
   * Serialization.
   */
  private static final long serialVersionUID = -1853915305418053143L;

  /**
   * Constructs a new NoSuchEmailException with null as its detail message.
   */
  public NoSuchEmailException() {

  }
  
  /**
   * Constructs a new NoSuchEmailException with its detail message.
   * @param message to be printed.
   */
  public NoSuchEmailException(String message) {
    super(message);
  }

}
